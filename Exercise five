from functools import reduce

fib_series = lambda n: reduce(lambda x, _: x+[x[-1]+x[-2]], range(n-2), [0, 1])

 print("Fibonacci series upto 1:") 
 print(fib_series(1))
 print("\nFibonacci series upto 3:") 
 print(fib_series(3))
 print("\nFibonacci series upto 7:")
 print(fib_series(7))
 print("\nFibonacci series upto 8:")
 print(fib_series(8))
